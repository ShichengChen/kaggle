    "    # mlp = load_model('pretrained/mlp_v4.h5')\n",
    "    \n",
    "    with open('pretrained/mlp_v4_hist.pkl', 'rb') as f:\n",
    "        hist = pickle.load(f)\n",
    "else:\n",
    "    sys.stdout = open('mlp_v4_out.txt', 'w')\n",
    "    fit = mlp.fit(xtr, ytr, batch_size=128, validation_data=(xte,yte),\n",
    "                  nb_epoch=40, verbose=1)\n",
    "    hist = fit.history"
   ]
  },
